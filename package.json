    "@types/three": "^0.182.0",
    "@vitest/coverage-v8": "^3.1.0",
    "glob": "^13.0.0",
    "react": "^19.0.0",
    "semantic-release": "^25.0.2",
    "three": "^0.182.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.0",
    "vitest": "^3.1.0",
    "yuka": "^0.7.8"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0.0",
    "@strata-game-library/core": ">=1.4.0",
